# Environment Configuration
# Copy this file to .env and adjust values as needed

# Application Environment
NODE_ENV=development

# Backend Configuration
PORT=43110
PYTHONPATH=./backend

# Ollama Configuration
OLLAMA_MODEL=llama3.2
OLLAMA_HOST=http://localhost:11434

# Frontend Configuration
REACT_APP_API_BASE=http://localhost:43110
REACT_APP_WS_URL=ws://localhost:43110/ws

# Logging
LOG_LEVEL=info
LOG_FILE=clippy-revival.log

# Security (generate your own in production)
# API_SECRET_KEY=your-secret-key-here

# Feature Flags
ENABLE_SCHEDULER=true
ENABLE_WEB_AUTOMATION=true
ENABLE_SOFTWARE_MANAGEMENT=true
ENABLE_CHARACTER_PACKS=true
ENABLE_VOICE_COMMANDS=false